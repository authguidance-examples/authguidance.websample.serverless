{
  "name": "basicspa",
  "version": "4.0.0",
  "description": "A Basic SPA using the OIDC Client security library and AWS Serverless Hosting",
  "main": "app.ts",
  "scripts": {
    "lint": "tslint -c tslint.json src/**/*.ts",
    "watch": "webpack --watch",
    "build": "webpack -p",
    "serve": "ts-node --no-cache server/server.ts",
    "package": "ts-node scripts/customPackager.ts",
    "deploy": "aws s3 cp .package s3://web.authguidance-examples.com --recursive",
    "clearcache": "aws cloudfront create-invalidation --distribution-id=E1KK6YUBAK08ZY --paths \"/*\""
  },
  "keywords": [],
  "author": "Gary Archer",
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.3.1",
    "oidc-client": "^1.5.4"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.0",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jquery": "^3.3.22",
    "@types/node": "^10.12.11",
    "core-js": "^2.5.7",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "https": "^1.0.0",
    "ts-loader": "^3.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1",
    "webpack": "^3.6.0"
  }
}

{
  "name": "sampleapi",
  "version": "4.0.0",
  "description": "An OAuth secured API using AWS Serverless Hosting via lambdas, and a portable coding model",
  "main": "handler.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts' && tslint -c tslint.json 'scripts/**/*.ts'",
    "build": "rimraf js && tsc",
    "authorizer": "sls invoke local -f authorizer -p test/authorizer.json",
    "getUserClaims": "sls invoke local -f getUserClaims -p test/getUserClaims.json",
    "getCompanyList": "sls invoke local -f getCompanyList -p test/getCompanyList.json",
    "getCompanyTransactions": "sls invoke local -f getCompanyTransactions -p test/getCompanyTransactions.json",
    "package": "rimraf .serverless && sls package",
    "customPackager": "ts-node deployment/customPackager.ts",
    "deploy": "sls deploy -p .serverless"
  },
  "author": "Gary Archer",
  "license": "ISC",
  "dependencies": {
    "framework-api-base": "file:src/framework-api-base",
    "framework-api-oauth": "file:src/framework-api-oauth",
    "fs-extra": "^8.1.0",
    "inversify": "^5.0.1",
    "middy": "^0.30.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.15",
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^12.11.7",
    "rimraf": "^2.6.2",
    "serverless": "^1.56.1",
    "serverless-deployment": "file:deployment",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  }
}

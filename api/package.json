{
  "name": "basicapi",
  "version": "1.0.0",
  "description": "A Basic OAuth secured API deployed as an AWS lambda",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json src/**/*.ts",
    "build": "ts-node --no-cache src/local.ts",
    "start": "nodemon -e ts --exec \"npm run build\"",
    "httpDebug": "cross-env HTTPS_PROXY=http://127.0.0.1:8888 nodemon -e ts --exec \"npm run build\"",
    "aws-compile": "ts-node scripts/clean.ts && tsc",
    "aws-files": "ts-node scripts/fileSetup.ts && cd .build && npm install && cd ..",
    "aws-package": "cd .build && zip -r deploy.zip * -x *.zip package*.json && cd ..",
    "aws-build": "npm run aws-build && npm run aws-files && npm run aws-package",
    "aws-deploy": "sls deploy"
  },
  "author": "Gary Archer",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "js-sha256": "^0.9.0",
    "memory-cache": "^0.2.0",
    "openid-client": "^2.2.1",
    "serverless-http": "^1.6.0",
    "tunnel-agent": "^0.6.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/memory-cache": "^0.2.0",
    "@types/node": "^10.5.6",
    "cross-env": "^5.2.0",
    "https": "^1.0.0",
    "nodemon": "^1.18.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}

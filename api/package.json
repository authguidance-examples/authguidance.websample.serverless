{
  "name": "sampleapi",
  "version": "4.0.0",
  "description": "An OAuth secured API using AWS Serverless Hosting via lambdas, and a portable coding model",
  "main": "handler.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts' && tslint -c tslint.json 'scripts/**/*.ts'",
    "build": "rimraf build && tsc",
    "auth": "sls invoke local -f authorize -p test/auth.json",
    "getUserClaims": "sls invoke local -f getUserClaims -p test/claims.json",
    "getCompanyList": "sls invoke local -f getCompanyList -p test/claims.json",
    "getCompanyTransactions": "sls invoke local -f getCompanyTransactions -p test/getCompanyTransactions.json",
    "package": "rimraf .serverless && sls package",
    "customPackager": "ts-node scripts/customPackager.ts",
    "deploy": "sls deploy -p .serverless"
  },
  "author": "Gary Archer",
  "license": "ISC",
  "dependencies": {
    "fs-extra": "^7.0.1",
    "jsonwebtoken": "^8.4.0",
    "jwks-rsa": "^1.3.0",
    "middy": "^0.19.2",
    "openid-client": "^2.4.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.15",
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.11",
    "adm-zip": "^0.4.13",
    "aws-sdk": "^2.368.0",
    "child-process-es6-promise": "^1.2.1",
    "rimraf": "^2.6.2",
    "serverless": "^1.56.1",
    "serverless-domain-manager": "^2.6.6",
    "serverless-plugin-scripts": "^1.0.2",
    "ts-node": "^6.2.0",
    "tslint": "^5.15.0",
    "tunnel-agent": "^0.6.0",
    "typescript": "^3.4.3"
  }
}
